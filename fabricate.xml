    	    -->
    	    <fns:project name="params.adligo.org" version="" />
    	    <fns:project name="params_tests.adligo.org" version="" />
    	    
    	    <!--  tests and tests_tests are deprecated, moving to tests4j -->
    	    <fns:project name="tests.adligo.org" version="" />
    	    <fns:project name="tests_tests.adligo.org" version="" />
    	    
    	    <fns:project name="tests4j.adligo.org" version="" />
