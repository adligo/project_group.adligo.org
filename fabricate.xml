<?xml version="1.0" encoding="UTF-8"?>
<fns:fabricate xmlns:fns="http://www.adligo.org/fabricate/xml/io_v1/fabricate_v1_0.xsd" 
    xmlns:lns="http://www.adligo.org/fabricate/xml/io_v1/library_v1_0.xsd" 
    xmlns:cns="http://www.adligo.org/fabricate/xml/io_v1/common_v1_0.xsd" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" >
    <!--  note the threads limiter is currently due to a issue
    	I am seeing with many ssh/git connections where I get a 
    	128 return code error, which needs to be looked into. 
     -->
    <fns:java Xmx="1g" Xms="256m" threads="8" />
    <fns:logs>
        <fns:log class="org.adligo.fabricate.external.RepositoryDownloader" setting="true"/>
        <fns:log class="org.adligo.fabricate.build.stages.MavenDownloader" setting="true"/>
        <fns:log class="org.adligo.fabricate.build.stages.GitObtainer" setting="true"/>
    </fns:logs>
    <fns:dependencies>
        <fns:remote_repository>http://127.0.0.1/</fns:remote_repository>
        <fns:remote_repository>http://repo1.maven.org/maven2/</fns:remote_repository>
        <fns:remote_repository>http://uk.maven.org/maven2/</fns:remote_repository>
        <fns:remote_repository>http://mirrors.ibiblio.org/maven2/</fns:remote_repository>
    </fns:dependencies>
    <fns:project_group>
    	<fns:stages>
    	    <fns:stage name="setup" class="org.adligo.fabricate.build.stages.DefaultSetup"/>
    	    <fns:stage name="obtain" class="org.adligo.fabricate.build.stages.GitObtainer"/>
    	    <fns:stage name="load" class="org.adligo.fabricate.build.stages.LoadAndCleanProjects"/>
    	    <fns:stage name="maven-obtain" class="org.adligo.fabricate.build.stages.MavenDownloader"/>
    	    <fns:stage name="jar" class="org.adligo.fabricate.build.stages.CompileJarAndDeposit">
    	    	<fns:task name="jar">
    	    	    <cns:param key="Default-Vendor" value="Adligo Inc"/>
    	    	</fns:task>  
    	    </fns:stage>
    	    <!-- these will be plugins and not part of the fabricate project itself
    	    <fns:stage name="test" class="org.adligo.fabricate.build.stages.Tests4jTests"/>
    	    <fns:stage name="publish" class="org.adligo.fabricate.build.stages.PublishToIntelligence4j"
    	        optional="true" />
    	         -->
    	</fns:stages>    
    	<fns:projects>
    	    <fns:scm>
    	        <fns:git hostname="git" path="/opt/git/org/" />
    	    </fns:scm>
    	    <!--  alpha order projects here, as the dependencies are figured out automatically -->
    	    <fns:project name="addoc.adligo.org" version=""/>
    	    <fns:project name="addoc_tests.adligo.org" version=""/>
    	    <!--  I will continue to support xml_io_generator using ant for a little while
    	    	although some new projects xml_io_generator_4ant and xml_io_generator_4fabricate 
    	    	will appear soon, to extract the ant specific code from xml_io_generator. 
    	     -->
    	    
    	    
    	    <fns:project name="ant_log.adligo.org" version=""/>
    	    <!-- 
    	    <fns:project name="ajaxb.adligo.org" version=""/>
    	    <fns:project name="ajaxb_tests.adligo.org" version=""/>
    	     -->
    	    <fns:project name="aws_client.adligo.org" version=""/>
    	    <fns:project name="aws_client_tests.adligo.org" version=""/>
    	    <!--  note awsgp and asdc* will be released later with different names. -->

    	    <fns:project name="cl.adligo.org" version=""/>
    	    <fns:project name="cl_tests.adligo.org" version=""/>

