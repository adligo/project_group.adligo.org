    	    <fns:project name="tests.adligo.org" version="" />
    	    <fns:project name="tests_tests.adligo.org" version="" />
    	    
    	    <fns:project name="tests4j.adligo.org" version="" />
    	    <fns:project name="tests4j_gen.adligo.org" version="" />
