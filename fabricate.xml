    	    <fns:project name="i_adig.adligo.org" version="" />
    	    <fns:project name="i_adig_tests.adligo.org" version="" />
    	    
    	    <fns:project name="i_db.adligo.org" version="" />
    	    <fns:project name="i_db_tests.adligo.org" version="" />
    	    
    	    <fns:project name="i_disk.adligo.org" version="" />
    	    <fns:project name="i_disk_tests.adligo.org" version="" />
    	    
    	    <fns:project name="i_ldap.adligo.org" version="" />
