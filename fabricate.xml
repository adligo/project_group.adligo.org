    	    <fns:project name="i_pool.adligo.org" version="" />
    	    <fns:project name="i_pool_tests.adligo.org" version="" />
    	    
    	    <fns:project name="i_smtp.adligo.org" version="" />
    	    <fns:project name="i_smtp_tests.adligo.org" version="" />
    	    
    	    <fns:project name="i_util.adligo.org" version="" />
    	    <fns:project name="i_util_tests.adligo.org" version="" />
    	    
    	    <fns:project name="jse_util.adligo.org" version="" />
